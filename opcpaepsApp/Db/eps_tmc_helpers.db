# Forward amphos setpoint records to fields
record(ao, "HPO:LPS:AMPHOS:MIN_VOLTAGE_RBV:FWD"){
  field(SCAN, ".1 second")
  field(DOL, "HPO:LPS:AMPHOS:MIN_VOLTAGE_RBV")
  field(OMSL, closed_loop)
  field(OUT, "HPO:LPS:AMPHOS:VOLTAGE_RBV.LOLO")
  field(FLNK, "HPO:LPS:AMPHOS:VOLTAGE_RBV")
}
record(ao, "HPO:LPS:AMPHOS:MIN_NOMINAL_VOLTAGE_RBV:FWD"){
  field(SCAN, ".1 second")
  field(DOL, "HPO:LPS:AMPHOS:MIN_NOMINAL_VOLTAGE_RBV")
  field(OMSL, closed_loop)
  field(OUT, "HPO:LPS:AMPHOS:VOLTAGE_RBV.LOW")
  field(FLNK, "HPO:LPS:AMPHOS:VOLTAGE_RBV") 
}

